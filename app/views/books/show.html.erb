<h1><%= @book.name %></h1>
<h2>Written By: <%= @book.author %></h2>
<h3>Ranked: <%= @book.rating %> </h3>
<%= @book.description %>
<%= button_to "Upvote", update_book_path(@book.id), method: :patch, class: "btn btn-primary" %>
<%= button_to "Edit", edit_book_path(@book.id), method: :get, class: "btn btn-primary" %>
<%= button_to "View all books", books_path, method: :get, class: "btn btn-primary" %>
<%= button_to "View all media", root_path, method: :get, class: "btn btn-primary" %>
