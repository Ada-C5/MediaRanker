<div class= "result">
  <h2> <%= @album.title %></h2>
  <p>Artist: <%= @album.artist %></p>
  <p>Ranking: <%= @album.rank %></p>
  <p>Description: <%= @album.description %></p>

  <%= button_to "Upvote", upvote_album_path(@album.id), method: :patch %>
<div>

<div class="buttons">
   <%= button_to "Edit Album", edit_album_path(@album.id), method: :get %>
   <%= button_to "DELETE", album_path, method: :delete %>
   <%= button_to "View All Albums", albums_path, method: :get %>
   <%= button_to "View All Media", root_path, method: :get %>
 </div>
