<table class="table table-hover">
  <thead>
    <tr>
      <th>
        Ranking
      </th>
      <th>
        Name
      </th>
      <th>
        Upvote
      </th>
    </tr> 
  </thead>
  <tbody>
      <% @albums.each do |album| %>
        <tr>
          <td>
            <%= album.rank %>
          </td>
          <td>
            <%= link_to album.title, album_path(album.id) %>
          </td>
          <td>
            Upvote Button 
          </td>
        </tr>
      <% end %>
  </tbody>
</table>