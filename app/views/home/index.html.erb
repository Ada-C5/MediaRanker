<div class="top-movies">
  <h3>Top Movies</h3>
  <ul>
    <% @movies.each do |movie| %>
      <li><%= link_to movie.title, movie_path(movie.id) %> Ranked: <%= movie.upvotes %></li>
    <% end %>
  </ul>
  <%= link_to "View More Movies", movies_path %>
</div>

