<div class="row">
  <div class="col-lg-12">
    <table class="table table-hover">
      <tbody>
        <tr>
          <th> Ranking </th>
          <th> Name </th>
          <th> Upvote </th>
        </tr>

        <% @movies.each do |movie| %>
          <tr>
            <td> Ranked: <%=movie.votes%> </td>
            <td> <%=movie.name%> </td>
            <td> Upvote Button </td>
          </tr>
        <% end %>

      </tbody>
    </table>
  </div>
</div>
