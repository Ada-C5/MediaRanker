<table class="table table-hover">
  <thead>
    <tr>
      <th>
        Ranking
      </th>
      <th>
        Name
      </th>
      <th>
        Upvote
      </th>
    </tr> 
  </thead>
  <tbody>
    <% @movies.each do |movie| %>
      <tr>
        <td>
          <%= movie.rank %>
        </td>
        <td>
          <%= link_to movie.title, movie_path(movie.id) %>
        </td>
        <td>
          Upvote Button 
        </td>
      </tr>
    <% end %>
  </tbody>
</table>